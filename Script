local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Player = game.Players.LocalPlayer
local Window = OrionLib:MakeWindow({Name = "Blox's Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "KeySystem"})
OrionLib:MakeNotification({
	Name = "Key_Blox's_Hub_8273127361272132",
	Content = "Sorry, but you need key "..Player.Name.."",
	Image = "rbxassetid://4483345998",
	Time = 5
})
_G.Key = "Key"
_G.KeyInput = "string"
function MakeScriptHub()
   local Window = OrionLib:MakeWindow({Name = "Blox's Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "Blox's Hub"})
end
function CorrectKeyNotification()
   OrionLib:MakeNotification({
	   Name = "Key Correct!",
	   Content = "You have entered the correct key!",
	   Image = "rbxassetid://4483345998",
	   Time = 5
   })
end
function IncorrectKeyNotification()
   OrionLib:MakeNotification({
	   Name = "Incorrect Key!",
	   Content = "You have entered the incorrect key!",
	   Image = "rbxassetid://4483345998",
	   Time = 5
   })
end
local Tab = Window:MakeTab({
	Name = "Key",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "Key System"
})
Tab:AddTextbox({
	Name = "Enter Key",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = Value
	end	  
})
Tab:AddButton({
	Name = "Check Key",
	Callback = function()
        if _G.KeyInput == _G.Key then
        MakeScriptHub()
        CorrectKeyNotification()
        else
           IncorrectKeyNotification()
      	end
  	end    
})
local Tab = Window:MakeTab({
	Name = "Credits",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
Tab:AddLabel("This Hub creat by Blox, from Orin Library")
